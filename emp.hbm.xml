<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
    <class table="Employee_IUD" name="Employee.EmpDAO">
        <id name="empID" type = "integer" column="EID">
            <generator class = "assigned"/>
        </id>
        <property name = "empName" type = "string" column = "ENAME" length = "20"/>
        <property name = "empSalary" type = "double" column = "ESAL" />
        <property name = "empGender" type = "string" column = "Gender" length = "20"/>
        
        
    </class>
</hibernate-mapping>